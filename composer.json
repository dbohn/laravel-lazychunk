{
    "name": "davidbohn/laravel-lazychunk",
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.6|^5.0|^6.0",
        "phpunit/phpunit": "^8.5",
        "laravel/legacy-factories": "^1.0.4"
    },
    "autoload": {
